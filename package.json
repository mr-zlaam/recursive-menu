{
  "name": "primelogicsolutions-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "concurrently \"eslint . --ext .js,.ts,.jsx,.tsx --max-warnings=0\"  \"pnpm run type-check\"",
    "lint:fix": "eslint --fix . --ext .js,.ts,.jsx,.tsx",
    "dev": "concurrently   \"pnpm fmt\"  \"pnpm lint  && next dev\"",
    "build": "pnpm run fmt && pnpm  type-check && pnpm lint && next build",
    "fmt": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "precommit": "concurrently \"pnpm run lint\" \"pnpm run fmt && git add .\"",
    "start": "next start",
    "commit-lint": "commitlint  --edit",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-menubar": "^1.1.3",
    "@radix-ui/react-navigation-menu": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@tabler/icons-react": "^3.26.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "clx": "^1.0.0",
    "concurrently": "^9.1.0",
    "framer-motion": "^11.14.4",
    "lucide-react": "^0.468.0",
    "next": "14.2.20",
    "nextjs-toploader": "^3.7.15",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.4.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.20",
    "husky": "^9.1.7",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
